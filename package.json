{
  "name": "track-your-peeps",
  "version": "1.0.0",
  "description": "Employee database management system.",
  "main": "index.js",
  "scripts": {
    "start": "npm run source && npm run seed && node server.js",
    "source": "mysql --defaults-extra-file=mysql.cnf < ./db/sql/schema.sql",
    "seed": "mysql --defaults-extra-file=mysql.cnf < ./db/sql/seeds.sql",
    "server": "node server.js"
  },
  "dependencies": {
    "console.table": "^0.10.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.0.1",
    "sequelize": "^6.28.0"
  }
}
